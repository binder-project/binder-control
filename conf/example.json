{
  "logging": {
    "host": "localhost",

    "logstash": {
      "port": 8050,
      "configDir": "~/binder-control/services/logging/logstash/"
    },

    "elasticsearch": {
      "port": 8052,
      "dir": "/data/binder/elasticsearch"
    },

    "kibana": {
      "port": 8053
    },

    "streaming": {
      "port": 2121
    }
  },

  "db": {
    "host": "localhost",
    "port": 9050,
    "dir": "/data/binder/db"
  },

  "kubernetes": {
    "port": 9051,
    "proxyPort": 8085
  },

 "binder-build": {
    "docker": {
      "enabled": true,
      "user": "N/A",
      "password": "N/A",
       "email": "N/A",
      "registry": "gcr.io/binder-testing",
      "host": "localhost"
    },
    "host": "localhost",
    "port": 8082
  },

  "binder-deploy-kubernetes": {
    "port": 8084,
    "kube": {
      "provider": "gce",
      "proxyHost": "localhost",
      "proxyPort": 8085,
      "apiVersion": "v1"
    },
    "pool": {
      "enabled": false,
      "size": 1
    },
    "registry": {
      "host": "localhost",
      "port": 8082
    },
    "test": {
      "templateDir": "examples/",
      "testing": false
    }
  },

  "binder-web": {
    "port": 2999,
    "build": {
      "host": "localhost",
      "port": 8082
    },
    "registry": {
      "host": "localhost",
      "port": 8082
    },
    "deploy": {
      "host": "localhost",
      "port": 8084
    }
  }
}

